<?xml version="1.0" encoding="utf-8"  standalone="no"?>
<Root Type="MenusResources" Version="1.0">
	<!-- 权限配置 说明：
		Project=>Key: 模块标识
		Project=>Name: 模块名称
		Project=>Ext: 扩展，暂不使用
		
		Menu=>Key: 唯一标识
		Menu=>Name: 名称
		Menu=>Path: 所在路径
		Menu=>Sequence: 显示顺序
		Menu=>Enable: 功能是否启用, true: 启用; false: 不启用
		Menu=>IsCommand: 是否是动作[按钮], true: 动作按钮, 放到t_function表中; false: 非动作按钮, 放到t_module表中
		Menu=>IconPath: 图标显示路径
		Menu=>Ext: 扩展字段，以键值对的方式存储：key:value,key:value
				   (1)sliderStatus:0|1|2|3|4   {参数说明：键——侧边栏状态 ,
				   值：0-无侧边栏；1-侧边栏；2-侧边栏 + 组织树，3-侧边栏 + 二级侧边栏，4-侧边栏 + 二级侧边栏 + 组织树}
		
		Key编码规则：ATMS_xx_xx_xx_xx  （ATMS_模块编码_子模块编码_功能编码_动作编码）
		             除前缀，以2位为单位，每增加一级添加2位
		             补充规则：菜单栏编码同上，功能或按钮规则改为 ATMS_fk_xx_xx_xx_xx
	-->
	<Project Key="ATMS_11" Name="流量分析" Ext="">
		<Menus>
			<!--子模块菜单 start -->
			<Menu Key="ATMS_11_00" ParentKey="ATMS_11"  Name="实时路况" Path="#/realTimeTraffic"
				  Sequence="0" Enable="true" IsCommand="false" IconPath=""  Ext="sliderStatus:0,iconY:36"/>
			<Menu Key="ATMS_11_01" ParentKey="ATMS_11"  Name="流量查询" Path="#/trafficQuery"
				  Sequence="1" Enable="true" IsCommand="false" IconPath=""  Ext="sliderStatus:0,iconY:37"/>
			<Menu Key="ATMS_09_00" ParentKey="ATMS_11"  Name="流量分布" Path="#/trafficFlowDistribution" Sequence="3" Enable="true" IsCommand="false" IconPath=""  Ext="sliderStatus:0,iconY:30"/>
			<Menu Key="ATMS_09_01" ParentKey="ATMS_11"  Name="车速分布" Path="#/speedDistribution" Sequence="4" Enable="true" IsCommand="false" IconPath=""  Ext="sliderStatus:0,iconY:31"/>
			<Menu Key="ATMS_09_02" ParentKey="ATMS_11"  Name="时间分布" Path="#/trendsAnalysis" Sequence="5" Enable="true" IsCommand="false" IconPath=""  Ext="sliderStatus:0,iconY:32"/>
			<Menu Key="ATMS_09_03" ParentKey="ATMS_11"  Name="进出城分析" Path="#/inOutCityAnalysis" Sequence="6" Enable="true" IsCommand="false" IconPath=""  Ext="sliderStatus:0,iconY:38"/>
			<Menu Key="ATMS_09_06" ParentKey="ATMS_11"  Name="车种比例" Path="#/carTypeProportion" Sequence="7" Enable="true" IsCommand="false" IconPath=""  Ext="sliderStatus:0,iconY:33"/>
			<Menu Key="ATMS_09_07" ParentKey="ATMS_11"  Name="供需对比" Path="#/supplyAndDemandComparison" Sequence="8" Enable="true" IsCommand="false" IconPath=""  Ext="sliderStatus:0,iconY:39"/>
			<Menu Key="ATMS_09_04" ParentKey="ATMS_11"  Name="OD分析" Path="#/ODAnalysis" Sequence="9" Enable="true" IsCommand="false" IconPath=""  Ext="sliderStatus:0,iconY:34"/>
			<Menu Key="ATMS_09_05" ParentKey="ATMS_11"  Name="密度分析" Path="#/densityAnalysis" Sequence="10" Enable="true" IsCommand="false" IconPath=""  Ext="sliderStatus:0,iconY:35"/>
			<!--子模块菜单 end -->
		</Menus>
	</Project>
</Root>
